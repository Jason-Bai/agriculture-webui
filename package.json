{
  "name": "agriculture-webui",
  "version": "1.0.0",
  "description": "本项目采用React、Redux开发，目的主要是通过此项目学习并掌握React、Redux的基本开发流程及使用技巧等。",
  "main": "index.js",
  "dependencies": {
    "antd": "0.12.12",
    "babel-polyfill": "^6.3.14",
    "bootstrap": "^3.3.7",
    "font-awesome": "^4.6.3",
    "isomorphic-fetch": "^2.1.1",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-redux": "^4.2.1",
    "react-router": "^2.7.0",
    "react-router-redux": "^4.0.5",
    "redux": "^3.2.1",
    "redux-logger": "^2.4.0",
    "redux-thunk": "^1.0.3"
  },
  "devDependencies": {
    "babel-core": "^6.3.15",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.1.1",
    "expect": "^1.6.0",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-webpack-plugin": "^2.22.0",
    "json-loader": "^0.5.4",
    "less-loader": "^2.2.3",
    "node-libs-browser": "^0.5.2",
    "redux-devtools": "^3.3.1",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.0.11",
    "webpack": "^1.9.11",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-hot-middleware": "^2.9.1"
  },
  "scripts": {
    "start": "[[ ${NODE_ENV} == \"production\" ]] && npm run start:prod || npm run start:dev",
    "start:dev": "rm -fr ./dist && npm run build:dev && node server.js",
    "start:prod": "rm -fr ./dist && npm run build:prod && node server.js",
    "build:dev": "webpack --debug --hot --progress --colors --display-error-details --config ./webpack.dev.js",
    "build:prod": "webpack --progress --colors --config ./webpack.prod.js"
  },
  "repository": {
